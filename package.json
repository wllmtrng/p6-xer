{
  "name": "p6-xer",
  "version": "1.0.0",
  "description": "A TypeScript module for parsing and processing Primavera P6 XER files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "xer-parser": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "node dist/cli.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "primavera",
    "p6",
    "xer",
    "parser",
    "excel",
    "xlsx"
  ],
  "author": "",
  "license": "ISC",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "commander": "^13.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "xlsx": "^0.18.5"
  }
}
